<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
	<application>
		<message-bundle>.messages_en_US</message-bundle>
		<message-bundle>.messages_es_ES</message-bundle>
		<message-bundle>.messages_pt_BR</message-bundle>
		<locale-config>
			<default-locale>pt_BR</default-locale>
			<supported-locale>en_US</supported-locale>
			<supported-locale>es_ES</supported-locale>
		</locale-config>
		<resource-bundle>
			<base-name>/messages</base-name>
			<var>msg</var>
		</resource-bundle>
	</application>

	<managed-bean>
		<managed-bean-name>linksMB</managed-bean-name>
		<managed-bean-class>ie.lyit.ccr.control.LinksMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>menuBarMB</managed-bean-name>
		<managed-bean-class>ie.lyit.ccr.control.MenuBarMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>certMB</managed-bean-name>
		<managed-bean-class>ie.lyit.ccr.control.CertificateMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>courseMB</managed-bean-name>
		<managed-bean-class>ie.lyit.ccr.control.CourseMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>postMB</managed-bean-name>
		<managed-bean-class>ie.lyit.ccr.control.PostMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>searchMB</managed-bean-name>
		<managed-bean-class>ie.lyit.ccr.control.SearchMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>tagCloudMB</managed-bean-name>
		<managed-bean-class>ie.lyit.ccr.control.TagCloudMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>userMB</managed-bean-name>
		<managed-bean-class>ie.lyit.ccr.control.UsersMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>userRolesMB</managed-bean-name>
		<managed-bean-class>ie.lyit.ccr.control.UserRolesMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>userCoursesMB</managed-bean-name>
		<managed-bean-class>ie.lyit.ccr.control.UserCoursesMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

</faces-config>
